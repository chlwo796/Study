package javaHigh05.example02;

import java.util.Map.Entry;
import java.util.TreeMap;

public class TreeMapEx02 {
	public static void main(String[] args) {
		TreeMap<String, String> treeMap1 = new TreeMap<String, String>();
		
		treeMap1.put("영업부", "강정호");
		treeMap1.put("홍보부", "김강인");
		treeMap1.put("기획부", "김영자");
		treeMap1.put("총무부", "김영자");
		treeMap1.put("인사부", "홍길동");
		treeMap1.put("회계부", "최재환");
		treeMap1.put("개발부", "최재환");

		
		for(Entry<String, String> e : treeMap1.entrySet()) {
			System.out.println(e);
		}
	}
}

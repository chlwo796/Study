package javaHigh03.example01;

import java.util.HashSet;
import java.util.Set;

public class SetEx02 {

	public static void main(String[] args) {
//		"사과", "배", "바나나", "귤", "배", "배"
//		1. Set 데이터 넣기
//		2. 전체출력
//		3. 크기출력
//		4. "참외"가 있는지 검색하기
		
		Set<String> fruits = new HashSet<String>();
		
		fruits.add("사과");
		fruits.add("배");
		fruits.add("바나나");
		fruits.add("귤");
		fruits.add("배");
		fruits.add("배");

	}

}

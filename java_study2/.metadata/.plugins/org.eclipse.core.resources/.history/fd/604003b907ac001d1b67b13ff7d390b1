		Iterator<Shop> it = shopSet.iterator();
		
		while(it.hasNext()) {
			Shop shop = it.next();
			System.out.println(shop.getCustname() + " " + shop.getCustPhone() + " " + shop.getPoint());
		}
		it = shopSet.iterator();
		while(it.hasNext()) {
			System.out.println(it.next());
		}
